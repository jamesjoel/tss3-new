
<%- include("../includes/header") %>

<div class="container my-5">
    <div class="row">
        <div class="col-md-12">
            <h2>Teacher Page</h2>
            <table class="table table-dark">
                <tr>
                    <th>S.No.</th>
                    <th>Name</th>
                    <th>Salary</th>
                    <th>Edit</th>
                    <th>Delete</th>
                </tr>
                <% result.forEach((value, index)=>{ %>
                    <tr>
                        <td><%= index+1 %></td>
                        <td><%= value.name %></td>
                        <td><%= value.salary %></td>
                        <td><a href="/teacher/edit/<%= value._id %>" class="btn btn-info btn-sm">Edit</a></td>
                        <td><a href="/teacher/delete/<%= value._id %>" class="btn btn-danger btn-sm">Delete</a></td>
                    </tr>
                <% }) %>
            </table>
        </div>
    </div>
</div>
</body>
</html>