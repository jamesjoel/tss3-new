<%- include("../include/header") %>

<div class="container-fluid my-5">
    <div class="row">
        <div class="col-md-8 offset-md-2 my-5">
            <table class="table table-dark table-striped">
                <tr> 
                    <th>S.no.</th>
                    <th>Full Name</th>
                    <th>Edit</th>
                    <th>Delete</th>

                </tr>
               <% result.forEach((value,index)=>{ %>
                    
                    <tr>
                        <td><%= index+1 %></td>
                        <td><%= value.fullname %></td>
                        <td><a href="/user/edit/<%= value._id %>" class="btn btn-info">Edit</a> </td>
                        <td><a href="/user/delete/<%= value._id %>" class="btn btn-danger">Delete</a> </td>

                    </tr>
                <% })%>
                
            
            </table>

        </div>
    </div>
</div>
</body>
</html>